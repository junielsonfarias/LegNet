{
  "name": "camara-mojui-dos-campos",
  "version": "1.0.0",
  "description": "Sistema completo da Câmara Municipal de Mojuí dos Campos - Portal Institucional e Painel Administrativo",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "ci:verify": "npm run lint && npm run test -- --runInBand",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-2025": "tsx prisma/seed-dados-2025.ts",
    "db:seed-ruropolis": "tsx prisma/seed-ruropolis.ts",
    "db:migrate-mock": "tsx prisma/migrate-from-mock.ts",
    "db:limpar": "tsx scripts/limpar-dados-camara.ts",
    "db:limpar:confirm": "tsx scripts/limpar-dados-camara.ts --confirm",
    "db:limpar:keep-admin": "tsx scripts/limpar-dados-camara.ts --keep-admin",
    "ci:readiness": "tsx scripts/readiness-check.ts",
    "verify:production": "tsx scripts/verify-production.ts",
    "monitor:readiness": "npm run ci:readiness",
    "load:tramitacoes": "k6 run load-tests/scripts/k6-tramitacoes.js",
    "metrics:sync": "curl -s -X POST \"$BASE_URL/api/monitoramento\"",
    "security:audit": "node scripts/security-audit.js",
    "security:audit:fix": "node scripts/security-audit.js --fix",
    "security:audit:json": "node scripts/security-audit.js --json",
    "security:check": "npm audit --audit-level=high"
  },
